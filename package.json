{
  "name": "collatz-prefixes",
  "version": "0.1.0",
  "description": "TypeScript implementation of Collatz Prefixes",
  "repository": "https://github.com/collatz-prefixes/collatz-prefixes-typescript.git",
  "homepage": "https://github.com/collatz-prefixes/collatz-prefixes-typescript#readme",
  "type": "module",
  "engines": {
    "node": ">=10.4.0"
  },
  "exports": {
    "types": "./dist/src/index.d.ts",
    "import": "./dist/src/index.mjs",
    "require": "./dist/src/index.cjs",
    "default": "./dist/src/index.mjs"
  },
  "targets": {
    "cjs": {
      "context": "node",
      "outputFormat": "commonjs",
      "isLibrary": true,
      "engines": {
        "node": ">=18"
      }
    },
    "mjs": {
      "context": "node",
      "outputFormat": "esmodule",
      "isLibrary": true,
      "engines": {
        "node": ">=18"
      }
    }
  },
  "files": [
    "dist/src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prestart": "yarn compile",
    "start": "node build/src/index.js",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "yarn compile",
    "pretest": "yarn compile",
    "test": "jest",
    "posttest": "yarn lint",
    "prepublish": "yarn compile"
  },
  "keywords": [
    "collatz",
    "mathematics"
  ],
  "author": "erhant",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^14.11.2",
    "babel-jest": "^29.5.0",
    "collatz-prefixes": "^0.1.0",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.0.3"
  }
}
