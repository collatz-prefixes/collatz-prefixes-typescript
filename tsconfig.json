{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "target": "ES2020",
    "esModuleInterop": true,
    "lib": ["ES2020"],
    "allowJs": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "types": ["jest"],
    "isolatedModules": true
  },
  "include": ["src/*.ts"]
}
